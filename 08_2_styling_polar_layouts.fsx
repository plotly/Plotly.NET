(**
## Styling the polar layout

Use the `Chart.withPolar` function and initialize a Polar layout with the desired looks

*)
open Plotly.NET.LayoutObjects

let styledPolar = 
    combinedPolar
    |> Chart.withPolar(
        Polar.init(
            Sector= (0., 270.),
            Hole=0.1
        )
    )(* output: 
